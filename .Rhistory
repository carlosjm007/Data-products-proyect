ggplot(data=data, aes(x=r)) +
geom_histogram(colour="#7b7b7b") +
xlim(c(-75,75)) +
scale_y_continuous(sec.axis = ~./600) +
geom_line(data = data, mapping = aes(x = x, y = y*600), color="median",size=2) +
geom_vline(xintercept=input$menos, color="mean",size=1) +
